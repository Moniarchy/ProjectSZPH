{
  "name": "SZPH",
  "version": "0.1.0",
  "private": true,
  "jest": {
    "testRegex": "(/tests/specs/.*|\\.(test|spec))\\.(js|jsx)$",
    "collectCoverage": "true",
    "coverageDirectory": "tests/coverage/",
    "coverageReporters":[ "lcov", "text" ],
    "coverageThreshold": {
      "global": {
        "branches": 80,
        "functions": 80,
        "lines": 80,
        "statements": 80
      }
    }
  },
  "devDependencies": {
    "react-scripts": "0.7.0"
  },
  "dependencies": {
    "chai": "^3.5.0",
    "enzyme": "^2.6.0",
    "jest": "^18.0.0",
    "react": "^15.3.2",
    "react-dom": "^15.3.2"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject"
  }
}
